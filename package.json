    "start": "node ./dist/server.js",
    "prestart": "npm run build:prod",
    "lint": "eslint --fix src/server/routes",
    "clean": "rimraf dist",
    "nodemon": "nodemon --exec babel-node ./dist/server.js",
    "dev": "npm-run-all -p clean nodemon watch:backEnd watch:frontEnd",
    "watch:backEnd": "webpack --watch --mode development --config webpack.config.server.js",
    "watch:frontEnd": "webpack --watch --mode development --config webpack.config.dev.js",
    "build:dev": "rimraf dist && webpack --mode development --config webpack.config.server.js && webpack --mode development --config webpack.config.dev.js",
    "build:prod": "rimraf dist && webpack --mode production --config webpack.config.server.js && webpack --mode production --config webpack.config.prod.js",
    "build:stats": "webpack --env production --json > stats.json",
    "watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
